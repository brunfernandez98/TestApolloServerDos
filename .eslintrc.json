{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "es6": true
    },
    "plugins": [
        "@typescript-eslint"
      ],
      "extends": [
        "eslint:recommended"
      ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": 0,
        "no-unused-vars": 2,
        "camelcase": "error",
        "comma-spacing": "error",
        "space-before-function-paren": ["error", "never"]

    }
    
}
