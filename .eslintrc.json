{
    "env": {
        "browser": true,
        "es2022": true,
        "node": true
    },
    "plugins": [
        "@typescript-eslint"
      ],
      "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
      ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "parser": "@typescript-eslint/parser"
    },
    "rules": {
        "no-console": 0,
        "no-unused-vars": 0
        

    }
    
}
